#!/usr/bin/env bash

# Iterate over files in the current directory
# Using a while loop to iterate over files
while IFS= read -r filename; do
    # Extract the second field from the file name using '-' as the delimiter
    second_field=$(echo "$filename" | cut -d '-' -f2)
    
    # Display the result
    echo "$second_field"
done < <(ls)
